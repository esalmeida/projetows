package br.projetows.servico;

import javax.jws.WebMethod;
import javax.jws.WebService;

@WebService
public class CotasRepositorioWS1 {
	CotasDAO cotasDAO;
	public CotasRepositorioWS1(){
		cotasDAO = new CotasDAO();
	}
	@WebMethod
	public Cotas consultaCotas (Cotas c){
		return cotasDAO.consulta(c);
	}
}